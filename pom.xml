<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>no.nav.fo</groupId>
    <artifactId>veilarbdialog</artifactId>
    <packaging>pom</packaging>

    <parent>
        <groupId>no.nav.sbl</groupId>
        <artifactId>mor-pom</artifactId>
        <version>3.2</version>
    </parent>

    <version>1</version>
    <modules>
        <module>api</module>
        <module>app</module>
        <module>rest</module>
        <module>ws</module>
        <module>config</module>
        <module>ear</module>
    </modules>

    <name>veilarbdialog</name>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>

        <!--Tjenester-->
        <dialogOppfoelging.tjenestespesifikasjon.version>1.0.0</dialogOppfoelging.tjenestespesifikasjon.version>
        <dial-nav-tjeneste-aktoer_v2.version>1.2</dial-nav-tjeneste-aktoer_v2.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>api-app-bom</artifactId>
                <version>LATEST</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>

            <!--Project dependencies-->
            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbdialog-config</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbdialog-api</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbdialog-app</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.fo</groupId>
                <artifactId>veilarbdialog-app</artifactId>
                <version>${project.version}</version>
                <scope>test</scope>
                <type>test-jar</type>
            </dependency>


            <!--Tjenester-->
            <dependency>
                <groupId>no.nav.tjenester</groupId>
                <artifactId>brukerdialog-dialogOppfoelging-v1-tjenestespesifikasjon</artifactId>
                <version>${dialogOppfoelging.tjenestespesifikasjon.version}</version>
                <classifier>jaxws</classifier>
            </dependency>
            <dependency>
                <groupId>no.nav.sbl.dialogarena</groupId>
                <artifactId>dial-nav-tjeneste-aktoer_v2</artifactId>
                <version>${dial-nav-tjeneste-aktoer_v2.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
</project>