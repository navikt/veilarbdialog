<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <context-param>
        <param-name>springKontekstKlasse</param-name>
        <param-value>no.nav.fo.veilarbdialog.ApplicationContext</param-value>
    </context-param>
    <listener>
        <listener-class>no.nav.apiapp.ApiAppServletContextListener</listener-class>
    </listener>

    <resource-ref>
        <res-ref-name>jdbc/DialogDS</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
</web-app>
